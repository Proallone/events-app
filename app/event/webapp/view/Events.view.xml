<mvc:View controllerName="com.proallone.event.controller.Events"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" showHeader="false">
        <List id="ProductList" items="{/Events}">
            <headerToolbar>
                <OverflowToolbar id="otMain">
                    <content>
                        <Title id="ttlMain" text="Events" level="H2"/>
                        <ToolbarSpacer id="tsMain" />
                    </content>
                </OverflowToolbar>
            </headerToolbar>
            <StandardListItem id="sliMain" title="{name}" description="{description}" icon="{image_url}" iconDensityAware="false" iconInset="false" press="handlePress" type="Navigation"/>
        </List>
    </Page>
</mvc:View>