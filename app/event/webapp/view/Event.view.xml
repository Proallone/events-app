<mvc:View controllerName="com.proallone.event.controller.Event"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="eventPage" title="Event" showNavButton="true" navButtonPress="navBack">
        <ObjectHeader icon="{image_url}" iconDensityAware="false" iconAlt="Denise Smith" imageShape="Circle" responsive="true" title="Denise Smith" intro="Senior Developer" class="sapUiResponsivePadding--header">
            <ObjectAttribute title="Name:" text="{name}" active="true" />
            <ObjectAttribute title="Description" text="{description}" />
            <ObjectAttribute title="Functional Area" text="Development" />
        </ObjectHeader>
    </Page>
</mvc:View>