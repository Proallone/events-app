        <tnt:mainContents>
            <Breadcrumbs currentLocationText="Laptop" separatorStyle="GreaterThan">
                <currentLocation>
                    <Link text="Events" press="onPress" />
                </currentLocation>
                <Link press="onPress" text="Suppliers"/>
            </Breadcrumbs>
            <App id="app"/>
        </tnt:mainContents>
    </tnt:ToolPage>
