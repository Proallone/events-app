<mvc:View controllerName="com.proallone.event.controller.App" displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core">
    <tnt:ToolPage id="toolPage" sideExpanded="false">
        <tnt:header>
            <tnt:ToolHeader id="_IDGenToolHeader1" design="Solid" height="4rem">
                <!-- <Button id="sideNavigationToggleButton" icon="sap-icon://menu2" type="Transparent" press="onSideNavButtonPress">
                    <layoutData>
                        <OverflowToolbarLayoutData priority="NeverOverflow" />
                    </layoutData>
                </Button> -->
                <Image id="_IDGenImage" src="test-resources/sap/tnt/images/SAP_Logo.png" decorative="false" press="onLogoPressed" tooltip="SAP Logo" width="80px" height="40px">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData" priority="NeverOverflow"/>
                    </layoutData>
                </Image>
                <Title id="_IDGenTitle" text="Community events" wrapping="false" class="title">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData1" priority="Disappear"/>
                    </layoutData>
                </Title>
                <ToolbarSpacer id="_IDGenToolbarSpacer" />
                <SearchField width="30rem" id="searchField" class="sapUiMediumMarginEnd">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData3" priority="Low" group="1"/>
                    </layoutData>
                </SearchField>
                <Avatar id="_IDGenAvatar" src="test-resources/sap/m/images/dronning_victoria.jpg" displaySize="S" press="onAvatarPressed" tooltip="Profile">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData2" priority="NeverOverflow"/>
                    </layoutData>
                </Avatar>
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:subHeader>
            <tnt:ToolHeader id="_IDGenToolHeader">
                <Breadcrumbs id="_IDGenBreadcrumbs" currentLocation="Events" separatorStyle="GreaterThan">
                    <Link id="_IDGenLink" text="Events" press="onBreadcrumbPress">
                        <customData>
                            <core:CustomData id="_IDGenCustomData" key="target" value="RouteEvents"/>
                        </customData>
                    </Link>

                </Breadcrumbs>
            </tnt:ToolHeader>
        </tnt:subHeader>
        <!-- <tnt:sideContent>
            <tnt:SideNavigation expanded="false" itemSelect="onItemSelect">
                <tnt:NavigationList>
                    <tnt:NavigationListItem text="Events" icon="sap-icon://calendar" enabled="true" selectable="true" key="key1"/>
                </tnt:NavigationList>
                <tnt:fixedItem>
                    <tnt:NavigationList>
                        <tnt:NavigationListItem text="About" icon="sap-icon://hint" press="onQuickActionPress" selectable="true"/>
                    </tnt:NavigationList>
                </tnt:fixedItem>
            </tnt:SideNavigation>
        </tnt:sideContent> -->
        <tnt:mainContents>
            <App id="app"/>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>